{
  "name": "express-mongo-typescript-api-boilerplate",
  "version": "1.0.0",
  "description": "Simple express typescript boilerplate",
  "main": "index.js",
  "scripts": {
    "build": "tsc --project ./tsconfig.json",
    "start": "yarn build && node build/index.js",
    "dev": "nodemon src/index.ts",
    "lint": "eslint ./src/*.ts",
    "clean": "rm -rf ./build"
